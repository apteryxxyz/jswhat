    },
    "main": "./dist/what.js",
    "files": [
        "dist",
        "bin"
    ],
    "bin": {
        "jswhat": "./bin/index.js"
    },
    "scripts": {
        "test": "npm run test:identify && npm run test:nontext",
        "test:identify": "node test/identify.test.js",
        "test:nontext": "node test/nontext.test.js",
        "eslint": "eslint ./src ./scripts ./bin/what --fix",
        "build": "npm run eslint && npm run test && npm run regexes && npm run webpack",
        "regexes": "npm run regexes:sync && npm run regexes:format && npm run regexes:combine",
        "regexes:format": "node scripts/format-regexes",
        "regexes:combine": "node scripts/combine-regexes",
        "regexes:sync": "node scripts/sync-regexes",
        "webpack": "webpack --config webpack.config.js",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/apteryxxyz/jswhat"
    },
    "bugs": {
        "url": "https://github.com/apteryxxyz/jswhat/issues"
    },
    "keywords": [
        "what",
        "jswhat",
        "pywhat",
        "cli",
        "node",
        "browser",
        "identify",
        "regex"
    ],
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.16.0",
        "babel-loader": "^8.2.3",
        "conventional-changelog-cli": "^2.1.1",
        "eslint": "^8.2.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "node-fetch": "2.6.5",
        "prettier": "^2.4.1",
        "webpack": "^5.62.1",
        "webpack-cli": "^4.9.1"
    }
}