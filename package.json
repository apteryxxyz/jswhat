{
    "name": "jswhat",
    "version": "1.0.0",
    "description": "Identify anything. jsWhat lets you easily identify emails, IP addresses, and more. Works in Node, Deno, the browser and the command line.",
    "author": {
        "name": "Apteryx",
        "url": "https://www.apteryx.xyz/"
    },
    "main": "dist/what.js",
    "license": "MIT",
    "homepage": "https://github.com/apteryxxyz/jswhat",
    "changelog": {
        "url": "https://github.com/apteryxxyz/jswhat/blob/CHANGELOG.md"
    },
    "bugs": {
        "url": "https://github.com/apteryxxyz/jswhat/issues"
    },
    "bin": {
        "what": "bin/what",
        "jswhat": "bin/what"
    },
    "scripts": {
        "test": "npx jest --verbose && npx es-check",
        "test:examples": "node examples/node.js && deno run --unstable --allow-read --allow-net examples/deno.js",
        "build": "npm run build:regexes && npm run build:browser && npm run build:update && npm run build:format && npm pack",
        "build:regexes": "node scripts/update-tlds.js && node scripts/format-json.js && node scripts/combine-regexes.js && node scripts/write-regexes.js",
        "build:browser": "node scripts/build-browser.js && npx gulp browserify",
        "build:format": "npx eslint --fix . && npx prettier --write --single-quote .",
        "build:update": "node scripts/update-values.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/apteryxxyz/jswhat.git"
    },
    "engines": {
        "node": ">=0.12.0"
    },
    "deprecated": false,
    "keywords": [
        "what",
        "jswhat",
        "pywhat",
        "cli",
        "node",
        "deno",
        "browser",
        "identify",
        "regex"
    ],
    "bundledDependencies": [],
    "devDependencies": {
        "@types/jest": "^26.0.23",
        "es-check": "^5.2.4",
        "eslint": "^7.30.0",
        "eslint-config-prettier": "^8.3.0",
        "gulp": "^4.0.2",
        "gulp-concat": "^2.6.1",
        "gulp-header": "^2.0.9",
        "gulp-uglify": "^3.0.2",
        "jest": "^27.0.4",
        "node-fetch": "^2.6.1",
        "prettier": "^2.3.2"
    }
}
